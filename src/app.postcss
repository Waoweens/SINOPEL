/* Write your global styles here, in PostCSS syntax */
html,
body {
	@apply h-full overflow-hidden;
}

.paper-container {
	width: calc(210 * 50%)
}

.paper-F4 {
	all: initial;
	display: block;
	box-sizing: border-box;
	width: 210mm;
	height: 330mm;
	font-family: 'Bookman Old Style', Georgia, Cambria, serif !important;
}

.paper-scale {
	transform: scale(0.5);
	transform-origin: 0 0;
}